    <div class="contact" id="signIn" style="height:1170px">
        <div id="particles-js2"></div>
        <div class="contact-top">

            <h3 class="title-w3 con">Sign In</h3>
            

            <form asp-controller="User" asp-action="Login" method="post" class="contact_form slideanim">
                <div class="message">
                    <div class="col-md-3 col-sm-3 grid_3 c1"></div>
                    <div class="col-md-6 col-sm-6 grid_6 c1">
                        <input type="text" class="margin-right" id="Username" Name="Username" placeholder="Enter Username or Email" required="">
                        <input type="password" Name="Password" id="Password" placeholder="Password" required="" style="width: 100%;border-radius: 0;line-height: 22px;font-size: 14px;padding: 14px 17px 14px;outline: none;color: #FFF;height: 50px;border: 1px solid #424141;margin: 0 0 20px;background: rgba(239, 235, 235, 0.18);letter-spacing: 1px;">
                        @{
                            if (ViewBag.LoginError != "")
                            {
                                <h4 style="color:white;">@ViewBag.LoginError</h4>
                            }
                        }
                    </div>

                    <div class="col-md-3 col-sm-3 grid_3 c1">
                    </div>
                    <div class="clearfix"></div>
                </div>

                <input type="submit" value="Sign In">
            </form>
            <form asp-controller="User" asp-action="Register" method="post" class="contact_form slideanim">
                <div class="message">
                    <div class="col-md-3 col-sm-3 grid_3 c1"></div>
                    <div class="col-md-6 col-sm-6 grid_6 c1">
                        <input type="text" class="margin-right" id="FirstName" Name="FirstName" placeholder="Enter First Name" required="">
                        <input type="text" class="margin-right" id="LastName" Name="LastName" placeholder="Enter Last Name" required="">
                        <input type="text" class="form-control pull-right" id="dateOfBirth" name="dateOfBirth" placeholder="Enter Date of Birth" required="">
                        <div class="form-group row">
                            <div class="col-md-1 col-sm-2 col-xs-6">
                                <label for="_GenderMale" style="color:white">
                                    <input type="radio" id="_GenderMale" name="_Gender" value="1" checked />
                                    Male
                                </label>
                            </div>
                            <div class="col-md-1 col-sm-2 col-xs-6">
                                <label for="_GenderFemale" style="color:white">
                                    <input type="radio" id="_GenderFemale" name="_Gender" value="2" />
                                    Female
                                </label>
                            </div>
                        </div>
                        <input type="text" class="margin-right" id="Email" Name="Email" placeholder="Enter Email" required="">
                        <input type="text" class="margin-right" id="Username" Name="Username" placeholder="Enter Username" required="">
                        <input type="password" Name="Password" id="Password" placeholder="Password" required="" style="width: 100%;border-radius: 0;line-height: 22px;font-size: 14px;padding: 14px 17px 14px;outline: none;color: #FFF;height: 50px;border: 1px solid #424141;margin: 0 0 20px;background: rgba(239, 235, 235, 0.18);letter-spacing: 1px;">
                        @{
                            if (ViewBag.RegisterMessage != "")
                            {
                                <h4 style="color:white;">@ViewBag.RegisterMessage</h4>
                            }
                        }
                    </div>

                    <div class="col-md-3 col-sm-3 grid_3 c1">
                    </div>
                    <div class="clearfix"></div>
                </div>

                <input type="submit" value="Sign Up">
            </form>
            
</div>
